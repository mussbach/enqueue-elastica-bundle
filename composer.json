    "description": "The bundle adds extra features to FOSElasticaBundle bundle",
    "keywords": ["elasticsearch", "elastica", "fos", "performance"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.0",
        "friendsofsymfony/elastica-bundle": "^7.0",
        "enqueue/enqueue-bundle": "^0.10"
    },
    "require-dev": {
        "doctrine/orm": "^2.0"
    },
    "autoload": {
        "psr-4": { "Enqueue\\ElasticaBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.10.x-dev"
        }
    }
}
